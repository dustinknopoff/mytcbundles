            "name": "a href",
            "supported_languages": [
                "html"
            ],
            "identifier": "html.a",
            "pattern": "a ${link:text}",
            "output_template": "<a href=\"${link}\">#</a>",
            "baseID": "24C243D2-3E13-4C62-A9B0-97C8C3848D89"
        },
        {
